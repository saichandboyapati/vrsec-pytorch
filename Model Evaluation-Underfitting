    {
      "cell_type": "code",
      "source": [
        "import torch\n",
        "import torch.nn as nn\n",
        "import torch.optim as optim\n",
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "\n",
        "# Generate non-linear data (parabolic function)\n",
        "torch.manual_seed(42)\n",
        "X = torch.linspace(-3, 3, 100).reshape(-1, 1)\n",
        "y = X**2 + 0.1 * torch.randn(X.shape)  # Quadratic function with noise\n",
        "\n",
        "plt.scatter(X.numpy(), y.numpy(), label=\"True Function\")\n",
        "plt.legend()\n",
        "plt.title(\"Non-Linear Data\")\n",
        "plt.show()"
      ],
      "metadata": {
        "id": "CbcKsN14f6e6",
        "outputId": "439ff613-73f3-48f0-e326-bab3e6de7597",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 452
        }
      },
      "execution_count": null,
      "outputs": [
        {
          "output_type": "display_data",
          "data": {
            "text/plain": [
              "<Figure size 640x480 with 1 Axes>"
            ],
            "image/png": "\n"
          },
          "metadata": {}
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "class SimpleLinearModel(nn.Module):\n",
        "    def __init__(self):\n",
        "        super(SimpleLinearModel, self).__init__()\n",
        "        self.linear = nn.Linear(1, 1)  # Single-layer (too simple for non-linear data)\n",
        "\n",
        "    def forward(self, x):\n",
        "        return self.linear(x)\n",
        "\n",
        "# Initialize model, loss function, and optimizer\n",
        "model = SimpleLinearModel()\n",
        "criterion = nn.MSELoss()\n",
        "optimizer = optim.SGD(model.parameters(), lr=0.01)\n"
      ],
      "metadata": {
        "id": "jLdO9LzRf7bE"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "epochs = 500\n",
        "losses = []\n",
        "\n",
        "for epoch in range(epochs):\n",
        "    optimizer.zero_grad()\n",
        "    y_pred = model(X)\n",
        "    loss = criterion(y_pred, y)\n",
        "    loss.backward()\n",
        "    optimizer.step()\n",
        "    losses.append(loss.item())\n",
        "\n",
        "# Plot loss\n",
        "plt.plot(losses)\n",
        "plt.xlabel(\"Epochs\")\n",
        "plt.ylabel(\"Loss\")\n",
        "plt.title(\"Training Loss Curve (Underfitting)\")\n",
        "plt.show()"
      ],
      "metadata": {
        "id": "4GWpFtkPhvfO",
        "outputId": "3b7b5ef4-3956-4338-a1d3-f64b57829f9e",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 472
        }
